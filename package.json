{
  "name": "ansi-tokenize",
  "version": "0.0.1",
  "description": "Split a string with ANSI escape codes into individual tokens",
  "author": {
    "name": "AlCalzone",
    "email": "d.griesel@gmx.net"
  },
  "license": "MIT",
  "type": "module",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "ansi-styles": "^6.2.1",
    "ava": "^5.2.0",
    "chalk": "^5.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "ansi-regex": "^6.0.1",
    "is-fullwidth-code-point": "^4.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "ava",
    "lint": "eslint ."
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "packageManager": "yarn@3.5.0"
}
